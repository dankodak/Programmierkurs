   "source": [
    "import math\n",
    "n=int(input(\"n=\"))\n",
    "i=0\n",
    "print(\"    x     |  sinx    |  Fehler  \")\n",
    "print(\"__________|__________|__________\")\n",
    "while i<n+1:\n",
    "    x=-0.5+(i/n)\n",
    "    sinx=math.sin(x)\n",
    "    Fehler=math.fabs(x-sinx)\n",
    "    print(\"{:^10.4f}|{:^10.4f}|{:^10.4f}\".format(x,sinx,Fehler))\n",
    "    i=i+1"
   ]
  }
